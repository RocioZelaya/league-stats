{
  "build": {
    "env": {
      "PYTHON_VERSION": "3.9.18"
    }
  },
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "stats-ui.html",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/fetch-data",
      "dest": "/api/index.py",
      "methods": ["GET"]
    },
    {
      "src": "/stats-ui.html",
      "dest": "/stats-ui.html"
    },
    {
      "src": "/",
      "dest": "/stats-ui.html"
    }
  ]
}
